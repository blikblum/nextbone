{
  "name": "nextbone",
  "version": "0.25.0",
  "description": "BackboneJS with ES classes and web components",
  "url": "https://github.com/blikblum/nextbone",
  "type": "module",
  "keywords": [
    "model",
    "view",
    "controller",
    "router",
    "server",
    "client",
    "browser"
  ],
  "author": "Luiz Américo Pereira Câmara",
  "main": "nextbone.js",
  "exports": {
    ".": "./nextbone.js",
    "./class-utils": "./class-utils.js",
    "./class-utils.js": "./class-utils.js",
    "./computed": "./computed.js",
    "./computed.js": "./computed.js",
    "./dom-utils": "./dom-utils.js",
    "./dom-utils.js": "./dom-utils.js",
    "./form": "./form.js",
    "./form.js": "./form.js",
    "./localstorage": "./localstorage.js",
    "./localstorage.js": "./localstorage.js",
    "./validation": "./validation.js",
    "./validation.js": "./validation.js",
    "./virtualcollection": "./virtualcollection.js",
    "./virtualcollection.js": "./virtualcollection.js"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "@esm-bundle/chai": "^4.3.4",
    "@open-wc/testing-helpers": "^0.7.22",
    "@rollup/plugin-babel": "^5.3.0",
    "@web/test-runner": "^0.13.12",
    "chai": "^4.3.4",
    "cross-env": "^5.2.0",
    "docco": "0.8.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "jquery": "^3.3.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-qunit": "^3.0.0",
    "karma-rollup-preprocessor": "^7.0.0",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0",
    "mocha": "^8.0.0",
    "prettier": "^1.16.4",
    "qunit": "^2.9.2",
    "rollup": "1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "test": "karma start test/core/karma.conf.cjs",
    "test:computed": "web-test-runner test/computed/*.js --node-resolve",
    "test:fetch": "mocha --require @babel/register test/fetch/setup.js test/fetch/*.js",
    "test:localstorage": "web-test-runner test/localstorage/*.js --node-resolve",
    "test:validation": "mocha --require @babel/register --require test/validation/setup-globals.js --ui exports --recursive test/validation/",
    "test:virtualcollection": "web-test-runner test/virtualcollection/*.js --node-resolve",
    "test:form": "web-test-runner test/form/*.js --node-resolve",
    "test:class-utils": "mocha --require @babel/register --require test/class-utils/setup-globals.js test/class-utils",
    "test:all": "npm run test && npm run test:computed && npm run test:fetch && npm run test:localstorage && npm run test:validation && npm run test:virtualcollection && npm run test:form && npm run test:class-utils",
    "doc": "docco backbone.js && docco examples/todos/todos.js examples/backbone.localStorage.js",
    "lint": "eslint nextbone.js computed.js localstorage.js virtualcollection.js form.js test/core/*.js",
    "format": "prettier nextbone.js computed.js localstorage.js virtualcollection.js form.js test/**/*.js --write"
  },
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextbone.git"
  },
  "files": [
    "nextbone.js",
    "computed.js",
    "localstorage.js",
    "validation.js",
    "virtualcollection.js",
    "form.js",
    "class-utils.js",
    "dom-utils.js",
    "LICENSE"
  ],
  "packageManager": "yarn@3.3.1"
}